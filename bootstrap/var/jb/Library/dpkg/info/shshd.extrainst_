#!/bin/sh

set -e

case "$1" in
    (install)
    /var/jb/bin/launchctl load -w /var/jb/Library/LaunchDaemons/us.diatr.shshd.plist
    ;;
	(upgrade)
	/var/jb/bin/launchctl unload /var/jb/Library/LaunchDaemons/us.diatr.shshd.plist
    /var/jb/bin/launchctl load /var/jb/Library/LaunchDaemons/us.diatr.shshd.plist
	;;
	(*)
	exit 0
	;;
esac

exit 0